<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Quirk Client Side</title>
    <link rel="stylesheet" href="/style/main.min.css" />
</head>

<body>


    <div class="modal-loader"></div>


    <div id="createCanvasModal"></div> <!-- create modal form to create canvas -->

    <div class="header" id="myHeader">
        <div class="d-flex">
            <div class="file_name_container">
                <div> File Name: <span id="file_name"></span> </div>
            </div>

            <div class="sub_header d-flex align-items-center ">

                <div class="dropdown dropdown-files files-container">
                    <li class="dropbtn dropbtn-files files-btn">Files</li>
                    <div id="myDropdown" class="dropdown-content">
                        <a id="save_json">Save</a>
                        <a id="preview-image">loop Name</a>


                        <a href="index.html" target="_blank">New Tab</a>

                    </div>
                </div>



                |

                <div class="align-canvas-container">
                    <button class="btn btn-sm btn-primary  align-canvas">
                        Align to canvas

                    </button>
                    <div class="align-canvas-buttons-container ">
                        <div id="horizontal">Horizontal</div>
                        <div id="vertical">Vertical</div>
                        <div id="center">Center</div>

                    </div>

                </div>
                <div>
                    <button id="add_name_btn" class="btn btn-sm btn-light">View Data</button>
                </div>
            </div>
            <div class="zoom_and_undo_container">


                <div class="zoom-container">
                    <button id="zoomOut">&#9866;</button>
                    VIEW
                    <button id="zoomIn">&#10010; </button>
                </div>


            </div>
        </div>

        <!-- tools -->
        <div class="tools">

            <div class="text">

                <div class="fonts-Select-size">
                    <select id="fontFamilySelect" class="fontFamilySelect  "></select>
                    <input type="number" value="18" id="fontSize"><br>

                </div>
                <div class="text-style-align">
                    <!-- color -->
                    <label for="color"> <img id="" width="15" alt="">
                    </label>
                    <input type="color" id="color" class="color  " value="#008080">

                    <!-- color -->
                    <label id="bold"> <img class=" "
                            src="data:image/svg+xml;base64,"
                            width="13" alt="">
                    </label>

                    <!-- italic -->
                    <label id="italic">
                        <img class=" "
                            src="data:image/svg+xml;base64,"
                            width="8" alt="" />
                    </label>

                    <!-- alignLeftText -->
                    <label id="alignLeftText">
                        <img class=" "
                            src="data:image/svg+xml;base64,"
                            width="15" alt="" />
                    </label>

                    <!-- alignCenterText -->
                    <label id="alignCenterText">
                        <img class=" "
                            src="data:image/svg+xml;base64,"
                            width="15" alt="" />
                    </label>
                    <!-- alignRightText -->
                    <label id="alignRightText">
                        <img class=" "
                            src="data:image/svg+xml;base64,"
                            alt="" width="15" />
                    </label>

                </div>
                <div class="fill-tools-container">



                    <div class="fill-icon" for="shapeFill">
                        <input type="color" id="shapeFill" value="#d6d6d6" class="shape-color  ">

                        <div>
                            <img src="images/canvas/transparent.png" id="removeColor" class="remove-color"
                                title="transparent" draggable="false" alt="">

                        </div>

                    </div>







                </div>
            </div>
            <div>


                <img id="align_left" src="images/canvas/align_left.png" title="align-left" width="25" draggable="false"
                    alt="">
                <img id="align_center" src="images/canvas/align_center.png" title="align-center" width="25"
                    draggable="false" alt="">
                <img id="align-right" src="images/canvas/align_right.png" title="align-right" width="25"
                    draggable="false" alt="">
                &nbsp;&nbsp;&nbsp;&nbsp;
                <img id="align-bottom" src="images/canvas/align_bottom.png" title="align-bottom" width="25"
                    draggable="false" alt="">
                <img id="align-middle" src="images/canvas/align_mid.png" title="align-middle" width="25"
                    draggable="false" alt="">
                <img id="align-top" src="images/canvas/align_top.png" title="align-top" width="25" draggable="false"
                    alt="">
            </div>
            <div class="insert-shape">
                <div class="dropdown dropdown-files insert-shape-div">
                    <li class="dropbtn dropbtn-files insert-shape-btn">Shape</li>
                    <div id="myDropdown" class="dropdown-content">
                        <a id="square"> Square</a>
                        <a id="circle">Circle</a>
                        <a>Rectangle</a>

                    </div>
                </div>

            </div>
            <li id="insert-text-btn" class="dropbtn-insert-text ">Insert Text</li>
        </div>





    </div>
    <!-- insert name -->
    <span class="excel-html-view-data" style="display: none;"></span>
    <div class="list-name-container">
        <div class="modal-header d-flex justify-content-start  bg-dark">

            <div>
                <div class="btn btn-sm btn-primary add-rows">+ Add Rows</div>
            </div>

            <div class=" swap-column d-flex justify-content-center gap-10 align-items-center btn btn-sm btn-primary">
                <img src="images/canvas/swap.png" width="15" alt="" class="swap-column">
                Swap Column



            </div>
            <div>
                <div class="btn btn-sm btn-primary clear-all">Clear rows</div>
            </div>

        </div>
        <div class="modal-content">

            <div class="textarea-container">
                <textarea class=""
                    placeholder="paste here the copied column data from excel or google spreadsheet"></textarea>

            </div>

            <div class="menu-controller d-flex justify-content-start">
                <div style="width: 300px;">Column 1</div>
                <div>Column 2</div>
            </div>

            <div class="list-names">

            </div>




        </div>
        <div class="bg-dark modal-footer d-flex justify-content-end">
            <div>
                <img src="./images/canvas/case.png" class="" id="sentence-case" width="23" alt="">

            </div>
            <div class="btn btn-sm btn-success save">
                Save and close
            </div>
        </div>
    </div>


    <!-- choose colum -->

    <div class="insert-textbox-container">


        <div class="modal-content ">
            <div class="d-flex align-items-center justify-content-between">

                <div>
                    <h2> Data 1</h2>
                </div>
                <div class="d-flex justify-content-end">
                    <div>
                        <div class="btn btn-sm btn-primary add-rows">+ Add Rows</div>
                    </div>


                    <div>
                        <div class="btn btn-sm btn-primary clear-all">Clear rows</div>
                    </div>
                </div>



            </div>

            <div class="textarea-container">
                <textarea class=""
                    placeholder="paste here the copied column data from excel or google spreadsheet"></textarea>

            </div>



            <div class="list-names">

            </div>








            <div class="d-flex justify-content-between   bg-dark modal-footer">
                <div>
                    <img src="./images/canvas/case.png" class="" id="sentence-case" width="23" alt="">

                </div>
                <div>
                    <div class="btn btn-sm btn-success text text-center generate-certificate-button ">
                        <h3>Generate</h3>
                    </div>
                    <div class="btn btn-sm btn-light text text-center close">
                        <h3>Close</h3>
                    </div>
                </div>


            </div>



        </div>



    </div>




    <div id="alert-header"></div>

    <main>

        <div id="canvas-background" class="canvas-container">


            <input type="file" name="myFile" class="drop-zone__input" style="display: none">


        </div>




    </main>


    <!-- stroke -->
    <!-- <div class="stroke-container">
                            <label>Stroke</label>
                            <input type="color" id="strokeColor" value="#059797">
                            <span class="label-range"><label>Width </label>
                                <span class="input-range"> <strong>-</strong> <input id="strokeWidth" max="200"
                                        type="range"><strong>+</strong> </span> </span>
                        </div> -->

    <!-- <div id="box-insert-tools">


                            <li id="upload_image">Insert Image</li>
            
                        </div> -->


    <div id="modal-container" class="modal-canvas">
        <div class="modal-content">
            <div class="modal-header d-flex justify-content-end">


                <div class="close-container bg-light p-1">
                    <div id="download_as_Zip" class="btn btn-sm btn-primary">
                        Download
                    </div>
                    <!-- <a id="printCanvas" class="btn btn-sm btn-light">Print</a> -->
                    <div>
                        <div class="btn btn-sm btn-dark close">Close</div>

                    </div>

                </div>
            </div>
            <div class="modal-body">

            </div>


        </div>
    </div>
    <!-- loader -->
    <div class="lds-spinner-container d-flex justify-content-center align-items-center ">
        <span class="loader">Loading Please wait</span>
    </div>


    <script src="javascript/canvas/library/jszip-utils.min.js"></script>


    <script src="javascript/canvas/library/jszip.min.js"></script>

    <script type="module" src="javascript/canvas/main.js"></script>
    <script src="javascript/canvas/library/fabric.js"></script>
    <script src="javascript/canvas/library/fonts.js"></script>

    <script>
        let template_json = '<%-template_json%>';
        let template_id = '<%-template_id%>';
        let canvas_image = '<%-canvas_image%>';
        let template_name = '<%-template_name%>';


    </script>

    <script src="javascript/canvas/test.js"></script>
</body>

</html>